@model IEnumerable<string>
@{ 
    Layout = null;
}
<ul>
    <p>
        @Html.ActionLink("All products", "List", "Product")
    </p>
    @if (Model != null)
    {
        foreach (var link in Model)
        {
        <p>
            @Html.RouteLink(link, new
    {
        controller = "Product",
        action = "List",
        category = link,
        page = 1
    },
    new
    {
        @class = link == ViewBag.SelectedCategory ? "selected" : null
    })
        </p>
        }
    }
</ul>
